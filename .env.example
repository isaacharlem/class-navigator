# This is an example environment file. Copy to .env.local and fill in your values.

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-at-least-32-chars

# Database - Using SQLite by default for development
DATABASE_URL="file:./dev.db"

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# Google Search API (if web search functionality is needed)
GOOGLE_API_KEY=your-google-api-key
GOOGLE_SEARCH_ENGINE_ID=your-search-engine-id

# Optional: Pre-created OpenAI Assistant ID for PDF processing
# If not provided, a new assistant will be created
OPENAI_PDF_ASSISTANT_ID=
